<h1>Search Users</h1>
{{#if error}} <span>{{error}}</span>{{/if}}
<form class="row g-2" method="POST" action="/user/search">
    <div class="col-auto">
        <input type="text" name="id" class="form-control" placeholder="Search User">
    </div>
    <div class="col-auto">
        <input type="submit" class="btn btn-primary" value="Search">
    </div>
    
</form>
